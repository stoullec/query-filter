{
    "name": "stoullec/query-filter",
    "version": "1.0.0",
    "description": "Convert url filter to dql to permit users to filter data with only one endpoint.",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Stoullec\\QueryFilter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "TOULLEC",
            "email": "simon-toullec@outlook.fr"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.15",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.20",
        "ramsey/uuid": "^4.7",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/event-dispatcher": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/validator": "6.2.*",
        "symfony/yaml": "6.2.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
