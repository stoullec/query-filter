{
    "name": "stoullec/query-filter",
    "description": "Convert url filter to dql to permit users to filter data with only one endpoint.",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Stoullec\\QueryFilter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "TOULLEC",
            "email": "simon-toullec@outlook.fr"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
